{
  "name": "hawkeye",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build-app": "babel app -d dist --copy-files",
    "build-static": "npm run build-dll-pro && npm run build-src",
    "start-dev": "npm run build-dll && npm run start-app",
    "start-app": "./node_modules/.bin/nodemon app/index.js & npm run watch-dev",
    "start": "NODE_ENV=production npm run build-src && ./node_modules/.bin/nodemon app/index.js",
    "dev": "./node_modules/webpack-dashboard/bin/webpack-dashboard.js -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config config/webpack/webpack.dev.js",
    "watch-dev": "./node_modules/webpack/bin/webpack.js --config config/webpack/webpack.dev.js --watch",
    "build-dll": "./node_modules/webpack/bin/webpack.js --config config/webpack/webpack.dll.js --progress --colors",
    "build-dll-pro": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config config/webpack/webpack.dll.js --progress --colors -p",
    "build-src": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config config/webpack/webpack.production.js -p",
    "deploy": "git checkout master && git pull origin master && npm i && npm run build-app && npm run build-static && pm2 update && npm run pm2",
    "pm2": "NODE_ENV=production pm2 start dist/index.js -i 3 --name \"hawkeye\" --max-memory-restart 300M --merge-logs --log-date-format=\"YYYY-MM-DD HH:mm Z\" --output=\"log/production.log\"",
    "pm2:restart": "NODE_ENV=production pm2 restart \"hawkeye\"",
    "pm2:stop": "NODE_ENV=production pm2 stop \"hawkeye\"",
    "pm2:logs": "NODE_ENV=production pm2 logs \"hawkeye\""
  },
  "author": "ecmadao",
  "license": "ISC",
